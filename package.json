{
  "name": "sudoku",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">= 20.14.0"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "tsc --project ./tsconfig.production.json && vite build",
