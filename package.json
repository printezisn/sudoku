  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/web-worker": "^1.6.0",
    "eslint": "^9.4.0",
    "globals": "^15.3.0",
    "jsdom": "^24.1.0",
    "sass": "^1.77.5",
    "typescript-eslint": "^8.0.0-alpha.24",
    "vitest": "^1.6.0"
  }
}
